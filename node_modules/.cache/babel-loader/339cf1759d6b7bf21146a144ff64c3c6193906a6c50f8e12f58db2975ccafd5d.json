{"ast":null,"code":"import { ref, onMounted, onUpdated, watch, inject } from 'vue';\nimport { storage } from '@/firebase/init.js';\nimport { ref as storageRef, getDownloadURL } from 'firebase/storage';\nimport axios from 'axios';\nimport { gsap, Power2 } from 'gsap';\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\ngsap.registerPlugin(ScrollTrigger);\nexport default {\n  name: 'CardList',\n  props: {\n    itemLimit: {\n      type: Number\n    }\n  },\n  setup(props) {\n    const emitter = inject('emitter');\n    const jsonData = ref({});\n    const displayedItems = ref([]);\n    const isDataCheck = ref(false);\n    const mymethod = () => {\n      emitter.emit('myevent', 100);\n    };\n    const firebaseData = async () => {\n      const storageInstance = storage;\n      const jsonFileRef = storageRef(storageInstance, `works.json`);\n      try {\n        const downloadURL = await getDownloadURL(jsonFileRef);\n        const res = await axios.get(downloadURL);\n        const workitems = res.data;\n        jsonData.value = workitems;\n        isDataCheck.value = true;\n        // console.log( props.customEventProp );\n        // props.customEventProp(true);\n        // emitter.emit('customEvent', 'Hello from YourComponent');\n      } catch (error) {\n        console.error(\"Error fetching data:\", error);\n      }\n    };\n    const videoDimds = () => {\n      const gnbItems = gsap.utils.toArray(\".video__dimd\");\n      gnbItems.forEach(item => {\n        let tl = gsap.timeline({\n          scrollTrigger: {\n            trigger: item\n          }\n        });\n        tl.set(item, {\n          autoAlpha: 1,\n          left: 0\n        });\n        tl.to(item, 0.75, {\n          left: \"100%\",\n          ease: Power2.out\n        });\n      });\n    };\n    const updateDisplayedItems = newVal => {\n      if (Array.isArray(newVal)) {\n        displayedItems.value = props.itemLimit !== undefined ? newVal.slice(0, props.itemLimit) : newVal;\n      }\n    };\n    watch(jsonData, updateDisplayedItems);\n    onMounted(() => {\n      firebaseData();\n    });\n    onUpdated(() => {\n      videoDimds();\n    });\n    return {\n      jsonData,\n      displayedItems,\n      videoDimds,\n      mymethod\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUpdated","watch","inject","storage","storageRef","getDownloadURL","axios","gsap","Power2","ScrollTrigger","registerPlugin","name","props","itemLimit","type","Number","setup","emitter","jsonData","displayedItems","isDataCheck","mymethod","emit","firebaseData","storageInstance","jsonFileRef","downloadURL","res","get","workitems","data","value","error","console","videoDimds","gnbItems","utils","toArray","forEach","item","tl","timeline","scrollTrigger","trigger","set","autoAlpha","left","to","ease","out","updateDisplayedItems","newVal","Array","isArray","undefined","slice"],"sources":["C:\\Rico\\portfilo2023\\src\\components\\Layouts\\List\\CardList.vue"],"sourcesContent":["<template>\r\n    <ul class=\"card--list\">\r\n        <li class=\"card__item\" v-for=\"(item, key) in  displayedItems\" :key=\"item.id\">\r\n            <router-link :to=\"{ name: 'WorkView', params: { id: item.id }}\">\r\n                <div class=\"video__dimd\"></div>\r\n                <video loop playsinline muted autoplay>\r\n                    <source :src=\"item.dataSrc\" type=\"video/mp4\">\r\n                </video>\r\n                <ul class=\"card--tag__list\">\r\n                <li>{{ item.skill[0] }}</li>\r\n                <li>{{ item.skill[1] }}</li>\r\n                </ul>\r\n            </router-link>\r\n        </li>\r\n    </ul>\r\n</template>\r\n      \r\n<script>\r\nimport { ref, onMounted, onUpdated, watch, inject } from 'vue';\r\nimport { storage } from '@/firebase/init.js';\r\nimport { ref as storageRef, getDownloadURL } from 'firebase/storage';\r\nimport axios from 'axios'\r\nimport { gsap, Power2 } from 'gsap';\r\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\r\ngsap.registerPlugin(ScrollTrigger);\r\n\r\nexport default {\r\n    name: 'CardList',\r\n    props: {\r\n        itemLimit: {\r\n            type: Number,\r\n        },\r\n   \r\n    },\r\n    setup(props) {\r\n\r\n        const emitter = inject('emitter');\r\n        const jsonData = ref({});\r\n        const displayedItems = ref([]);\r\n        const isDataCheck = ref( false );\r\n        const mymethod = () => {\r\n            emitter.emit('myevent', 100);\r\n        };\r\n        const firebaseData = async () => {\r\n            const storageInstance = storage;\r\n            const jsonFileRef = storageRef(storageInstance, `works.json`);\r\n\r\n            try {\r\n                const downloadURL = await getDownloadURL(jsonFileRef);\r\n                const res = await axios.get(downloadURL);\r\n                const workitems = res.data;\r\n                jsonData.value = workitems;\r\n                isDataCheck.value = true;\r\n                // console.log( props.customEventProp );\r\n                // props.customEventProp(true);\r\n                // emitter.emit('customEvent', 'Hello from YourComponent');\r\n            } catch (error) {\r\n                console.error(\"Error fetching data:\", error);\r\n            }\r\n        };\r\n\r\n        const videoDimds = () => {\r\n            const gnbItems = gsap.utils.toArray(\".video__dimd\");\r\n            gnbItems.forEach((item) => {\r\n                let tl = gsap.timeline({\r\n                    scrollTrigger: {\r\n                        trigger: item,\r\n                    }\r\n                });\r\n                tl.set(item, { autoAlpha: 1, left: 0 });\r\n                tl.to(item, 0.75, {\r\n                    left: \"100%\",\r\n                    ease: Power2.out\r\n                });\r\n            });\r\n        };\r\n\r\n        const updateDisplayedItems = (newVal) => {\r\n            if (Array.isArray(newVal)) {\r\n                displayedItems.value = props.itemLimit !== undefined\r\n                    ? newVal.slice(0, props.itemLimit)\r\n                    : newVal;\r\n            }\r\n        };\r\n\r\n        watch(jsonData, updateDisplayedItems);\r\n\r\n   \r\n        onMounted(() => {\r\n            firebaseData();\r\n        });\r\n        onUpdated( () =>{\r\n            videoDimds();\r\n        })\r\n \r\n        return {\r\n            jsonData,\r\n            displayedItems,\r\n            videoDimds,\r\n            mymethod\r\n        };\r\n    }\r\n}\r\n</script>\r\n      \r\n<style lang=\"scss\" scoped>\r\n@import \"@/assets/scss/__variables.scss\";\r\n\r\n.card {\r\n    &--list {\r\n        >li {\r\n            position: relative;\r\n            margin: vw(0 0 50px 0);\r\n            border-radius: vw(15px);\r\n            overflow: hidden;\r\n            height: vw(750px);\r\n\r\n            a {\r\n                display: block;\r\n                height: inherit;\r\n\r\n                .video__dimd {\r\n                    position: absolute;\r\n                    top: 0;\r\n                    left: 0;\r\n                    width: 100%;\r\n                    height: 100%;\r\n                    background: #fff;\r\n                    z-index: 5;\r\n                }\r\n\r\n                video {\r\n                    width: 100%;\r\n                    height: 100%;\r\n                    object-fit: cover;\r\n                    pointer-events: none;\r\n                    position: absolute;\r\n                    top: 0;\r\n                    left: 0;\r\n                    z-index: 0;\r\n                }\r\n            }\r\n        }\r\n    }\r\n\r\n    &--tag__list {\r\n        position: absolute;\r\n        bottom: vw(50px);\r\n        left: vw(30px);\r\n        display: flex;\r\n\r\n        >li {\r\n            border-radius: vw(30px);\r\n            font-size: vw(40px);\r\n            padding: vw(14px 30px);\r\n            margin-left: vw(20px);\r\n            background: #fff;\r\n            color: $dpColor;\r\n            font-weight: 600;\r\n\r\n            &:first-child {\r\n                margin-left: 0;\r\n            }\r\n\r\n            &:nth-child(odd) {\r\n                background: $dpColor;\r\n                color: #fff;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n@media (max-width: 960px) {\r\n    .card {\r\n        &--tag__list {\r\n            >li{\r\n                font-size:vw(60px);\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n      "],"mappings":"AAkBA,SAASA,GAAG,EAAEC,SAAS,EAAEC,SAAS,EAAEC,KAAK,EAAEC,MAAK,QAAS,KAAK;AAC9D,SAASC,OAAM,QAAS,oBAAoB;AAC5C,SAASL,GAAE,IAAKM,UAAU,EAAEC,cAAa,QAAS,kBAAkB;AACpE,OAAOC,KAAI,MAAO,OAAM;AACxB,SAASC,IAAI,EAAEC,MAAK,QAAS,MAAM;AACnC,SAASC,aAAY,QAAS,oBAAoB;AAClDF,IAAI,CAACG,cAAc,CAACD,aAAa,CAAC;AAElC,eAAe;EACXE,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACHC,SAAS,EAAE;MACPC,IAAI,EAAEC;IACV;EAEJ,CAAC;EACDC,KAAKA,CAACJ,KAAK,EAAE;IAET,MAAMK,OAAM,GAAIf,MAAM,CAAC,SAAS,CAAC;IACjC,MAAMgB,QAAO,GAAIpB,GAAG,CAAC,CAAC,CAAC,CAAC;IACxB,MAAMqB,cAAa,GAAIrB,GAAG,CAAC,EAAE,CAAC;IAC9B,MAAMsB,WAAU,GAAItB,GAAG,CAAE,KAAM,CAAC;IAChC,MAAMuB,QAAO,GAAIA,CAAA,KAAM;MACnBJ,OAAO,CAACK,IAAI,CAAC,SAAS,EAAE,GAAG,CAAC;IAChC,CAAC;IACD,MAAMC,YAAW,GAAI,MAAAA,CAAA,KAAY;MAC7B,MAAMC,eAAc,GAAIrB,OAAO;MAC/B,MAAMsB,WAAU,GAAIrB,UAAU,CAACoB,eAAe,EAAG,YAAW,CAAC;MAE7D,IAAI;QACA,MAAME,WAAU,GAAI,MAAMrB,cAAc,CAACoB,WAAW,CAAC;QACrD,MAAME,GAAE,GAAI,MAAMrB,KAAK,CAACsB,GAAG,CAACF,WAAW,CAAC;QACxC,MAAMG,SAAQ,GAAIF,GAAG,CAACG,IAAI;QAC1BZ,QAAQ,CAACa,KAAI,GAAIF,SAAS;QAC1BT,WAAW,CAACW,KAAI,GAAI,IAAI;QACxB;QACA;QACA;MACJ,EAAE,OAAOC,KAAK,EAAE;QACZC,OAAO,CAACD,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;MAChD;IACJ,CAAC;IAED,MAAME,UAAS,GAAIA,CAAA,KAAM;MACrB,MAAMC,QAAO,GAAI5B,IAAI,CAAC6B,KAAK,CAACC,OAAO,CAAC,cAAc,CAAC;MACnDF,QAAQ,CAACG,OAAO,CAAEC,IAAI,IAAK;QACvB,IAAIC,EAAC,GAAIjC,IAAI,CAACkC,QAAQ,CAAC;UACnBC,aAAa,EAAE;YACXC,OAAO,EAAEJ;UACb;QACJ,CAAC,CAAC;QACFC,EAAE,CAACI,GAAG,CAACL,IAAI,EAAE;UAAEM,SAAS,EAAE,CAAC;UAAEC,IAAI,EAAE;QAAE,CAAC,CAAC;QACvCN,EAAE,CAACO,EAAE,CAACR,IAA