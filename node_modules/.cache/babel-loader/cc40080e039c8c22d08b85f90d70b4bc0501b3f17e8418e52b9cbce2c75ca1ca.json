{"ast":null,"code":"import { storage } from '@/firebase/init.js';\nimport { ref as storageRef, getDownloadURL } from 'firebase/storage';\nimport axios from 'axios';\nimport { reactive, ref, onMounted, onUpdated, watch } from 'vue';\nimport { gsap, Power2 } from 'gsap';\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\ngsap.registerPlugin(ScrollTrigger);\nexport default {\n  name: 'CardList',\n  props: {\n    itemLength: {\n      type: Number\n    },\n    workJsonItem: {\n      type: Object,\n      required: true\n    }\n  },\n  setup() {\n    const jsonData = reactive({});\n    const firebaseData = async () => {\n      const storageInstance = storage;\n      const jsonFileRef = storageRef(storageInstance, `works.json`);\n      try {\n        const downloadURL = await getDownloadURL(jsonFileRef);\n        const res = await axios.get(downloadURL);\n        const workitems = res.data;\n        isCheck.value = true;\n        Object.assign(jsonData, workitems);\n      } catch (error) {\n        console.error(\"Error fetching data:\", error);\n      }\n    };\n    onMounted(() => {\n      firebaseData();\n      const gnbItems = gsap.utils.toArray(\".video__dimd\");\n      gnbItems.forEach(item => {\n        let tl = gsap.timeline({\n          scrollTrigger: {\n            trigger: item\n          }\n        });\n        tl.set(item, {\n          autoAlpha: 1,\n          left: 0\n        });\n        tl.to(item, 0.75, {\n          left: 100 + \"%\",\n          ease: Power2.out\n        });\n      });\n    });\n    return {\n      isCheck,\n      jsonData\n    };\n  }\n};","map":{"version":3,"names":["storage","ref","storageRef","getDownloadURL","axios","reactive","onMounted","onUpdated","watch","gsap","Power2","ScrollTrigger","registerPlugin","name","props","itemLength","type","Number","workJsonItem","Object","required","setup","jsonData","firebaseData","storageInstance","jsonFileRef","downloadURL","res","get","workitems","data","isCheck","value","assign","error","console","gnbItems","utils","toArray","forEach","item","tl","timeline","scrollTrigger","trigger","set","autoAlpha","left","to","ease","out"],"sources":["C:\\Rico\\portfilo2023\\src\\components\\Layouts\\List\\CardList.vue"],"sourcesContent":["<template>\r\n    <ul class=\"card--list\">\r\n        <li class=\"card__item\" v-for=\"(item, key) in workItems\" :key=\"key\">\r\n            <router-link :to=\"{ name: 'WorkView', params: { id: item.id }}\">\r\n                <div class=\"video__dimd\"></div>\r\n                <video loop playsinline muted autoplay>\r\n                <source :src=\"item.dataSrc\" type=\"video/mp4\">\r\n                </video>\r\n                <ul class=\"card--tag__list\">\r\n                <li>{{ item.skill[0] }}</li>\r\n                <li>{{ item.skill[1] }}</li>\r\n                </ul>\r\n            </router-link>\r\n        </li>\r\n    </ul>\r\n    <!-- <ul class=\"card--list\">\r\n        <li class=\"card__item\" v-for=\"(item, index) in workItems.slice(0, itemLength)\" :key=\"index\">\r\n            <router-link :to=\"{name: 'WorkView', params: { id: item.id }}\">\r\n                <div class=\"video__dimd\"></div>\r\n                <video loop playsinline muted autoplay>\r\n                    <source :src=\"item.dataSrc\" type=\"video/mp4\">\r\n                </video>\r\n                <ul class=\"card--tag__list\" >\r\n                    <li>{{ item.skill[0] }}</li>\r\n                    <li>{{ item.skill[1] }}</li>\r\n                </ul>\r\n            </router-link>>\r\n        </li> \r\n    </ul> -->\r\n</template>\r\n      \r\n<script>\r\nimport { storage } from '@/firebase/init.js';\r\nimport { ref as storageRef, getDownloadURL } from 'firebase/storage';\r\nimport axios from 'axios'\r\nimport { reactive, ref, onMounted, onUpdated, watch } from 'vue';\r\nimport { gsap, Power2 } from 'gsap';\r\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\r\ngsap.registerPlugin(ScrollTrigger);\r\n\r\nexport default {\r\n    name: 'CardList',\r\n    props: {\r\n        itemLength: {\r\n            type: Number,\r\n        },\r\n        workJsonItem: {\r\n            type: Object,\r\n            required: true\r\n        },\r\n    },\r\n    setup() {\r\n        const jsonData = reactive({});\r\n        const firebaseData = async () => {\r\n            const storageInstance = storage;\r\n            const jsonFileRef = storageRef(storageInstance, `works.json`);\r\n            try {\r\n                const downloadURL = await getDownloadURL(jsonFileRef);\r\n                const res = await axios.get( downloadURL );\r\n                const workitems = res.data; \r\n                isCheck.value = true;\r\n                Object.assign(jsonData, workitems);\r\n            } catch (error) {\r\n                console.error(\"Error fetching data:\", error);\r\n            }\r\n        } \r\n        onMounted(() => {\r\n            firebaseData();\r\n            const gnbItems = gsap.utils.toArray(\".video__dimd\");\r\n            gnbItems.forEach((item) => {\r\n                let tl = gsap.timeline({\r\n                    scrollTrigger: {\r\n                        trigger: item,\r\n                    }\r\n                });\r\n                tl.set(item, { autoAlpha: 1, left: 0 });\r\n                tl.to(item, 0.75, {\r\n                    left: 100 + \"%\",\r\n                    ease: Power2.out\r\n                });\r\n            })\r\n        })\r\n        return {\r\n            isCheck,\r\n            jsonData\r\n        };\r\n    }\r\n}\r\n</script>\r\n      \r\n<style lang=\"scss\" scoped>\r\n@import \"@/assets/scss/__variables.scss\";\r\n\r\n.card {\r\n    &--list {\r\n        >li {\r\n            position: relative;\r\n            margin: vw(0 0 50px 0);\r\n            border-radius: vw(15px);\r\n            overflow: hidden;\r\n            height: vw(750px);\r\n\r\n            a {\r\n                display: block;\r\n                height: inherit;\r\n\r\n                .video__dimd {\r\n                    display: none;\r\n                    position: absolute;\r\n                    top: 0;\r\n                    left: 0;\r\n                    width: 100%;\r\n                    height: 100%;\r\n                    background: #fff;\r\n                    z-index: 5;\r\n                }\r\n\r\n                video {\r\n                    width: 100%;\r\n                    height: 100%;\r\n                    object-fit: cover;\r\n                    pointer-events: none;\r\n                    position: absolute;\r\n                    top: 0;\r\n                    left: 0;\r\n                    z-index: 0;\r\n                }\r\n            }\r\n        }\r\n    }\r\n\r\n    &--tag__list {\r\n        position: absolute;\r\n        bottom: vw(50px);\r\n        left: vw(30px);\r\n        display: flex;\r\n\r\n        >li {\r\n            border-radius: vw(30px);\r\n            font-size: vw(40px);\r\n            padding: vw(14px 30px);\r\n            margin-left: vw(20px);\r\n            background: #fff;\r\n            color: $dpColor;\r\n            font-weight: 600;\r\n\r\n            &:first-child {\r\n                margin-left: 0;\r\n            }\r\n\r\n            &:nth-child(odd) {\r\n                background: $dpColor;\r\n                color: #fff;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n@media (max-width: 960px) {\r\n    .card {\r\n        &--tag__list {\r\n            >li{\r\n                font-size:vw(60px);\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n      "],"mappings":"AAgCA,SAASA,OAAM,QAAS,oBAAoB;AAC5C,SAASC,GAAE,IAAKC,UAAU,EAAEC,cAAa,QAAS,kBAAkB;AACpE,OAAOC,KAAI,MAAO,OAAM;AACxB,SAASC,QAAQ,EAAEJ,GAAG,EAAEK,SAAS,EAAEC,SAAS,EAAEC,KAAI,QAAS,KAAK;AAChE,SAASC,IAAI,EAAEC,MAAK,QAAS,MAAM;AACnC,SAASC,aAAY,QAAS,oBAAoB;AAClDF,IAAI,CAACG,cAAc,CAACD,aAAa,CAAC;AAElC,eAAe;EACXE,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACHC,UAAU,EAAE;MACRC,IAAI,EAAEC;IACV,CAAC;IACDC,YAAY,EAAE;MACVF,IAAI,EAA