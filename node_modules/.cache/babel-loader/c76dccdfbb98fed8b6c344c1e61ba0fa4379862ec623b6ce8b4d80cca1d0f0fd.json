n        \r\n\r\n        return {\r\n            startIndex,\r\n            endIndex,\r\n            currentPage,\r\n            // paginatedPineapples,\r\n            prevPage,\r\n            nextPage,\r\n            prevProjectUrl,\r\n            nextProjectUrl,\r\n            nextInfoProject\r\n        };\r\n    }\r\n\r\n}\r\n\r\n\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n@import \"@/assets/scss/__variables.scss\";\r\n\r\n.work_project {\r\n    .project_info {\r\n        a {\r\n            padding: vw(50px 100px);\r\n            display: block;\r\n            width: 100%;\r\n            background: url(\"@/assets/images/nextproject_img1.jpg\") 0 0 no-repeat;\r\n            background-size:cover;\r\n            height: vw(260px);\r\n            box-sizing: border-box;\r\n\r\n            em {\r\n                display: block;\r\n            }\r\n\r\n            .text {\r\n                font-size: vw(30px);\r\n                font-weight: 600;\r\n                color: #fff;\r\n            }\r\n\r\n            .title {\r\n                padding-top: vw(30px);\r\n                text-align: center;\r\n                font-size: vw(50px);\r\n                color: #fff;\r\n                font-family: 'Noto Sans Korean';\r\n                font-weight: bold;\r\n            }\r\n        }\r\n    }\r\n\r\n    &.on {\r\n        display: flex;\r\n        justify-content: space-between;\r\n\r\n        .project_info {\r\n            width: 50%;\r\n        }\r\n    }\r\n}\r\n</style>"],"mappings":"AAqBA,SAASA,GAAG,EAAEC,KAAK,EAAEC,QAAQ,EAAEC,SAAQ,QAAW,KAAK;AACvD,SAASC,YAAW,QAAS,eAAc;AAE3C,MAAOC,eAAc,GAAID,YAAY,CAAC,CAAC;AAEvC,eAAe;EACXE,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE,CACP,CAAC;EACDC,KAAKA,CAACD,KAAK,EAAE;IAAEE;EAAK,CAAC,EAAE;IACnB,MAAMC,UAAS,GAAIR,QAAQ,CAAC,MAAMG,eAAe,CAACK,UAAU,CAAC;IAC7D,MAAMC,QAAO,GAAIT,QAAQ,CAAC,MAAMG,eAAe,CAACM,QAAQ,CAAC;IACzD,MAAMC,WAAU,GAAIV,QAAQ,CAAC,MAAMG,eAAe,CAACO,WAAW,CAAC;IAG/D,MAAMC,QAAO,GAAIA,CAAA,KAAM;MACnB,IAAID,WAAW,CAACE,KAAI,GA