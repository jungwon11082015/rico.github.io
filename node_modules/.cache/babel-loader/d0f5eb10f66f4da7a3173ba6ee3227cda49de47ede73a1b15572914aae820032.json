ProjectInfo.value = props.nextPageData;\r\n            console.log( nextProjectInfo.value );\r\n            // prevProjectInfo.value = props.prevPageData;\r\n            // nextProjectInfo.value = props.nextPageData;\r\n            // console.log( \"projectInfoChage\", prevProjectInfo.value, nextProjectInfo.value );\r\n        }\r\n\r\n        onMounted(() => {\r\n\r\n            projectInfoChage();\r\n        });\r\n\r\n        return {\r\n            showPrevLink,\r\n            showNextLink,\r\n            emitPageChange,\r\n            nextProjectInfo,\r\n            prevProjectInfo\r\n        };\r\n    }\r\n\r\n}\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n@import \"@/assets/scss/__variables.scss\";\r\n\r\n.work_project {\r\n    .project_info {\r\n        a {\r\n            padding: vw(50px 100px);\r\n            display: block;\r\n            width: 100%;\r\n            background: url(\"@/assets/images/nextproject_img1.jpg\") 0 0 no-repeat;\r\n            background-size: 100% auto;\r\n            height: vw(260px);\r\n            box-sizing: border-box;\r\n\r\n            em {\r\n                display: block;\r\n            }\r\n\r\n            .text {\r\n                font-size: vw(30px);\r\n                font-weight: 600;\r\n                color: #fff;\r\n            }\r\n\r\n            .title {\r\n                padding-top: vw(30px);\r\n                text-align: center;\r\n                font-size: vw(50px);\r\n                color: #fff;\r\n                font-family: 'Noto Sans Korean';\r\n                font-weight: bold;\r\n            }\r\n        }\r\n    }\r\n\r\n    &.on {\r\n        display: flex;\r\n        justify-content: space-between;\r\n\r\n        .project_info {\r\n            width: 50%;\r\n\r\n            a {\r\n                background-size: auto 100%;\r\n                background-position: center center;\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>"],"mappings":"AAiBA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,KAAK,