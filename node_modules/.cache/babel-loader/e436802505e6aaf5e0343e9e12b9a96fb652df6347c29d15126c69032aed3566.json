 async () => {\r\n            try {\r\n                const response = await axios.get(\"https://www.tistory.com/apis/post/read?\", {\r\n                    params: {\r\n                        access_token: ACCESS_TOKEN,\r\n                        blogName: 'rico86',\r\n                        page: '1',\r\n                    },\r\n                });\r\n                console.log( response );\r\n                // responseItems.value = response.data.tistory.item.posts;\r\n                // count.value =  response.data.tistory.item.posts.length;\r\n                // console.log( responseInfo.value );\r\n            } catch (error) {\r\n                console.error('An error occurred:', error);\r\n            }\r\n        };\r\n        \r\n\r\n\r\n        \r\n        onMounted(() => {\r\n            tstoryList();\r\n            tstoryInfo();\r\n        });\r\n        \r\n        return {\r\n            responseItems,\r\n            count,\r\n            tstoryList\r\n        };\r\n    }\r\n}\r\n</script>\r\n  \r\n<style lang=\"scss\" scoped>\r\n@import \"@/assets/scss/__variables.scss\";\r\n\r\n#blog {\r\n    .inner {\r\n        display: block;\r\n\r\n        h2 {\r\n            padding-top: 150px;\r\n            text-align: center;\r\n            font-weight: bold;\r\n            font-size: 120px;\r\n            color: $dpColor;\r\n        }\r\n\r\n        .blog--word {\r\n            padding: 20px 0;\r\n            text-align: center;\r\n            font-size: 24px;\r\n            font-family: 'Noto Sans Korean';\r\n            font-weight: 600;\r\n            color: #181818;\r\n\r\n            >em {\r\n                font-size: 24px;\r\n                color: $dpColor;\r\n\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n  "],"mappings":"AAWA,OAAOA,QAAO,MAAO,wCAAuC;AAC5D,SAASC,GAAG,EAAEC,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AAC9C,OAAOC,KAAI,MAAO,OAAM;AAGxB,eAAe;EACXC,IAAI,EAAE,MAAM;EACZC,KAAK,EAAE;IACPC,YAAY,EAAE;MACVC,IAAI,EAAEC,KAAK;MACXC,QAAQ,EAAE;IACV;E