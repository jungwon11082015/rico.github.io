{"ast":null,"code":"import { ref, inject, onMounted, watch } from 'vue';\nimport CardList from '@/components/Layouts/List/CardList.vue';\nimport AllWork from '@/components/Layouts/buttons/AllWork.vue';\nimport Skeletor from \"@/components/Layouts/common/Skeleton.vue\";\nexport default {\n  name: 'Work',\n  components: {\n    CardList,\n    AllWork,\n    Skeletor\n  },\n  setup() {\n    const isShow = ref(false);\n    const emitter = inject('emitter');\n    const isCheck = ref(false);\n    const loadDataChek = newVal => {\n      console.log(newVal);\n      if (newVal) {\n        console.log(\"여기까지 타니?\");\n      }\n      emitter.on('workDataJson', value => {\n        console.log('isDataShow received!', `value: ${value}`);\n        isCheck.value = value;\n        // isCheck.value = true;\n      });\n    };\n\n    watch(isShow, loadDataChek);\n    onMounted(() => {\n      isShow.value = true;\n      // loadDataChek();\n    });\n\n    return {\n      isShow,\n      isCheck\n    };\n  }\n};","map":{"version":3,"names":["ref","inject","onMounted","watch","CardList","AllWork","Skeletor","name","components","setup","isShow","emitter","isCheck","loadDataChek","newVal","console","log","on","value"],"sources":["C:\\Rico\\portfilo2023\\src\\views\\Main\\Section\\Work.vue"],"sourcesContent":["<template>\r\n    <section class=\"card\" >\r\n        <div class=\"inner\"  > \r\n            <h2>Project Work</h2>\r\n            <template v-if=\"isCheck\">\r\n                <CardList  :itemLimit=\"5\" />\r\n                </template> \r\n                <template v-else>\r\n                    <Skeletor :SkeletorLength=\"5\" :SkeletorLocation=\"`main`\"  :SkeletorType=\"`list`\"/>\r\n                </template> \r\n            <AllWork /> \r\n        </div>  \r\n    </section>\r\n</template>\r\n       \r\n<script>\r\nimport { ref, inject, onMounted, watch   } from 'vue';\r\nimport CardList from '@/components/Layouts/List/CardList.vue'\r\nimport AllWork from '@/components/Layouts/butt