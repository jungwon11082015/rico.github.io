{"ast":null,"code":"import { ref, onMounted, computed, watch } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport { storage } from '@/firebase/init.js';\nimport { ref as storageRef, getDownloadURL } from 'firebase/storage';\nimport axios from 'axios';\nimport Pagination from '@/components/Layouts/common/Pagination.vue';\nexport default {\n  name: 'worksView',\n  props: {},\n  components: {\n    Pagination\n  },\n  setup(props) {\n    const route = useRoute();\n    const router = useRouter();\n    const jsonData = ref({});\n    const currentPage = ref(1);\n    const totalPages = ref(0);\n    const prevId = computed(() => {\n      return route.params.id > 0 ? route.params.id - 1 : null;\n    });\n    const nextId = computed(() => {\n      return route.params.id < totalPages.value - 1 ? route.params.id + 1 : null;\n    });\n    console.log(prevId, nextId);\n    const firebaseData = async () => {\n      const storageInstance = storage;\n      const jsonFileRef = storageRef(storageInstance, `works.json`);\n      try {\n        const downloadURL = await getDownloadURL(jsonFileRef);\n        const res = await axios.get(downloadURL);\n        const workitems = res.data;\n        const curtPage = workitems[route.params.id];\n        totalPages.value = workitems.length;\n        jsonData.value = workitems[route.params.id];\n        currentViewPage(curtPage.id - 1);\n      } catch (error) {\n        console.error(\"Error fetching data:\", error);\n      }\n    };\n    const changePage = page => {\n      currentPage.value = page;\n    };\n    const currentViewPage = curtPage => {\n      currentPage.value = curtPage;\n    };\n    onMounted(() => {\n      firebaseData();\n      currentViewPage();\n    });\n    return {\n      jsonData,\n      routeParamsId: route.params.id,\n      currentPage,\n      totalPages,\n      prevId,\n      nextId,\n      changePage\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","watch","useRoute","useRout