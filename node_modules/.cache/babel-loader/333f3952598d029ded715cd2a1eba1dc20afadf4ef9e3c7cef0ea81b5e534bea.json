{"ast":null,"code":"import { ref, onMounted, reactive } from 'vue';\nimport { storage } from '@/firebase/init.js';\nimport { ref as storageRef, getDownloadURL } from 'firebase/storage';\nimport axios from 'axios';\nimport CardList from '@/components/Layouts/List/CardList.vue';\nimport AllWork from '@/components/Layouts/buttons/AllWork.vue';\nexport default {\n  name: 'Work',\n  // props: {\n  //     itemLength: {\n  //         type: Number,\n  //         default: 5\n  //     },\n  //     workJsonItem: {\n  //         type: Object,\n  //         required: true\n  //     }\n  // },\n  components: {\n    CardList,\n    AllWork\n  },\n  setup() {\n    const isCheck = ref(false);\n    const jsonData = reactive({});\n    const firebaseData = async () => {\n      const storageInstance = storage;\n      const jsonFileRef = storageRef(storageInstance, `works.json`);\n      try {\n        const downloadURL = await getDownloadURL(jsonFileRef);\n        const res = await axios.get(downloadURL);\n        const workitems = res.data;\n        isCheck.value = true;\n        Object.assign(jsonData, workitems);\n      } catch (error) {\n        console.error(\"Error fetching data:\", error);\n      }\n    };\n    onMounted(() => {\n      firebaseData();\n    });\n    return {\n      isCheck,\n      jsonData\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","reactive","storage","storageRef","getDownloadURL","axios","CardList","AllWork","name","components","setup","isCheck","jsonData","firebaseData","storageInstance","jsonFileRef","downloadURL","res","get","workitems","data","value","Object","assign","error","console"],"sources":["C:\\Rico\\portfilo2023\\src\\views\\Main\\Section\\Work.vue"],"sourcesContent":["<template>\r\n    <section class=\"card\" >\r\n        <div class=\"inner\"  > \r\n            <h2>Project Work</h2>\r\n            <template v-if=\"isCheck && isCheck.length > 0\">\r\n                <CardList  :workJsonItem=\"jsonData\" :itemLength=\"5\" />\r\n   