{"ast":null,"code":"import { defineEmits } from 'vue';\nimport { ref, onMounted, onUpdated, watch } from 'vue';\nimport { storage } from '@/firebase/init.js';\nimport { ref as storageRef, getDownloadURL } from 'firebase/storage';\nimport axios from 'axios';\nimport { gsap, Power2 } from 'gsap';\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\ngsap.registerPlugin(ScrollTrigger);\nexport default {\n  name: 'CardList',\n  props: {\n    itemLimit: {\n      type: Number\n    }\n  },\n  setup(props) {\n    console.log(\"props전달받았는지 체크===>\", props);\n    const jsonData = ref({});\n    const displayedItems = ref([]);\n    const hasProps = ref(!!props);\n    const firebaseData = async () => {\n      const storageInstance = storage;\n      const jsonFileRef = storageRef(storageInstance, `works.json`);\n      try {\n        const downloadURL = await getDownloadURL(jsonFileRef);\n        const res = await axios.get(downloadURL);\n        const workitems = res.data;\n        jsonData.value = workitems;\n      } catch (error) {\n        console.error(\"Error fetching data:\", error);\n      }\n    };\n    const videoDimds = () => {\n      const gnbItems = gsap.utils.toArray(\".video__dimd\");\n      gnbItems.forEach(item => {\n        let tl = gsap.timeline({\n          scrollTrigger: {\n            trigger: item\n          }\n        });\n        tl.set(item, {\n          autoAlpha: 1,\n          left: 0\n        });\n        tl.to(item, 0.75, {\n          left: 100 + \"%\",\n          ease: Power2.out\n        });\n      });\n    };\n    watch(jsonData, newVal => {\n      console.log(hasProps.value);\n      if (Array.isArray(newVal)) {\n        displayedItems.value = newVal.slice(0, props.itemLimit);\n        console.log(displayedItems.value.length);\n      }\n    });\n    onUpdated(() => {\n      videoDimds();\n    });\n    onMounted(() => {\n      firebaseData();\n    });\n    return {\n      jsonData,\n      displayedItems,\n      videoDimds\n    };\n  }\n};","map":{"version":3,"names":["defineEmits","ref","onMounted","onUpdated","watch","storage","storageRef","getDownloadURL","axios","gsap","Power2","ScrollTrigger","registerPlugin","name","props","itemLimit","type","Number","setup","console","log","jsonData","displayedItems","hasProps","firebaseData","storageInstance","jsonFileRef","downloadURL","res","get","workitems","data","value","error","videoDimds","gnbItems","utils","toArray","forEach","item","tl","timeline","scrollTrigger","trigger","set","autoAlpha","left","to","ease","out","newVal","Array","isArray","slice","length"],"sources":["C:\\Rico\\portfilo2023\\src\\components\\Layouts\\List\\CardList.vue"],"sourcesContent":["<template>\r\n    <ul class=\"card--list\">\r\n        <li class=\"card__item\" v-for=\"(item, key) in  displayedItems\" :key=\"item.id\">\r\n            <router-link :to=\"{ name: 'WorkView', params: { id: item.id }}\">\r\n                <div class=\"video__dimd\"></div>\r\n                <video loop playsinline muted autoplay>\r\n                    <source :src=\"item.dataSrc\" type=\"video/mp4\">\r\n                </video>\r\n                <ul class=\"card--tag__list\">\r\n                <li>{{ item.skill[0] }}</li>\r\n                <li>{{ item.skill[1] }}</li>\r\n                </ul>\r\n            </router-link>\r\n        </li>\r\n    </ul>\r\n</template>\r\n      \r\n<script>\r\nimport { defineEmits } from 'vue';\r\nimport { ref, onMounted, onUpdated, watch } from 'vue';\r\nimport { storage } from '@/firebase/init.js';\r\nimport { ref as storageRef, getDownloadURL } from 'firebase/storage';\r\nimport axios from 'axios'\r\nimport { gsap, Power2 } from 'gsap';\r\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\r\ngsap.registerPlugin(ScrollTrigger);\r\n\r\nexport default {\r\n    name: 'CardList',\r\n    props: {\r\n        itemLimit: {\r\n            type: Number,\r\n        },\r\n    },\r\n    setup(props) {\r\n        \r\n        console.log( \"props전달받았는지 체크===>\",  props );\r\n\r\n\r\n        const jsonData = ref({});\r\n        const displayedItems = ref([]);\r\n        const hasProps = ref(!!props);\r\n\r\n        const firebaseData = async () => {\r\n            const storageInstance = storage;\r\n            const jsonFileRef = storageRef(storageInstance, `works.json`);\r\n\r\n            try {\r\n                const downloadURL = await getDownloadURL(jsonFileRef);\r\n                const res = await axios.get( downloadURL );\r\n                const workitems = res.data;\r\n                jsonData.value =  workitems;\r\n            } catch (error) {\r\n                console.error(\"Error fetching data:\", error);\r\n            }\r\n        } \r\n        const videoDimds = () => {\r\n            const gnbItems = gsap.utils.toArray(\".video__dimd\");\r\n            gnbItems.forEach((item) => {\r\n                let tl = gsap.timeline({\r\n                    scrollTrigger: {\r\n                        trigger: item,\r\n                    }\r\n                });\r\n                tl.set(item, { autoAlpha: 1, left: 0 });\r\n                tl.to(item, 0.75, {\r\n                    left: 100 + \"%\",\r\n                    ease: Power2.out\r\n                });\r\n            })\r\n        }\r\n\r\n\r\n        watch(jsonData, (newVal) => {\r\n            console.log( hasProps.value );\r\n\r\n            if (Array.isArray(newVal)) {\r\n                displayedItems.value = newVal.slice(0, props.itemLimit);\r\n                console.log(displayedItems.value.length );\r\n            }\r\n        });\r\n\r\n\r\n        onUpdated(() => {\r\n            videoDimds();\r\n        });\r\n\r\n        onMounted(() => {\r\n            firebaseData();\r\n        })\r\n        return {\r\n            jsonData,\r\n            displayedItems,\r\n            videoDimds,\r\n        };\r\n    }\r\n}\r\n</script>\r\n      \r\n<style lang=\"scss\" scoped>\r\n@import \"@/assets/scss/__variables.scss\";\r\n\r\n.card {\r\n    &--list {\r\n        >li {\r\n            position: relative;\r\n            margin: vw(0 0 50px 0);\r\n            border-radius: vw(15px);\r\n            overflow: hidden;\r\n            height: vw(750px);\r\n\r\n            a {\r\n                display: block;\r\n                height: inherit;\r\n\r\n                .video__dimd {\r\n                    position: absolute;\r\n                    top: 0;\r\n                    left: 0;\r\n                    width: 100%;\r\n                    height: 100%;\r\n                    background: #fff;\r\n                    z-index: 5;\r\n                }\r\n\r\n                video {\r\n                    width: 100%;\r\n                    height: 100%;\r\n                    object-fit: cover;\r\n                    pointer-events: none;\r\n                    position: absolute;\r\n                    top: 0;\r\n                    left: 0;\r\n                    z-index: 0;\r\n                }\r\n            }\r\n        }\r\n    }\r\n\r\n    &--tag__list {\r\n        position: absolute;\r\n        bottom: vw(50px);\r\n        left: vw(30px);\r\n        display: flex;\r\n\r\n        >li {\r\n            border-radius: vw(30px);\r\n            font-size: vw(40px);\r\n            padding: vw(14px 30px);\r\n            margin-left: vw(20px);\r\n            background: #fff;\r\n            color: $dpColor;\r\n            font-weight: 600;\r\n\r\n            &:first-child {\r\n                margin-left: 0;\r\n            }\r\n\r\n            &:nth-child(odd) {\r\n                background: $dpColor;\r\n                color: #fff;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n@media (max-width: 960px) {\r\n    .card {\r\n        &--tag__list {\r\n            >li{\r\n                font-size:vw(60px);\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n      "],"mappings":"AAkBA,SAASA,WAAU,QAAS,KAAK;AACjC,SAASC,GAAG,EAAEC,SAAS,EAAEC,SAAS,EAAEC,KAAI,QAAS,KAAK;AACtD,SAASC,OAAM,QAAS,oBAAoB;AAC5C,SAASJ,GAAE,IAAKK,UAAU,EAAEC,c