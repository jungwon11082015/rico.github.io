{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport { gsap, Expo } from 'gsap';\nimport scrollEvent from '@/assets/js/scrollEvent';\nexport default {\n  name: 'Header',\n  mixins: [scrollEvent],\n  props: {},\n  setup() {\n    const sT = ref(0);\n    const rotateValue = ref(0);\n    const isOpen = ref(false);\n    const logo = ref(null);\n    const gnbTarget = ref(null);\n    onMounted(() => {\n      handleScroll();\n    });\n    const handleScroll = () => {\n      sT.value = window.scrollY;\n      rotateValue.value = sT.value / 10;\n      logo.value.$el.style.transform = 'rotate(' + Match.floor(rotateValue.value) + 'deg)';\n      console.log(logo.value.$el.style.transform);\n    };\n    const gnbOpen = () => {\n      isOpen.value = !isOpen.value;\n      const body = document.body;\n      if (isOpen.value) {\n        gsap.to(gnbTarget.value, 0.05, {\n          y: 0,\n          ease: Expo.easeIn\n        });\n        body.style.overflow = \"hidden\";\n        // if( logo.value.$el.style.transform  < )\n        console.log(logo.value.$el.style.transform);\n      } else {\n        gsap.to(gnbTarget.value, 0.05, {\n          y: -101 + \"%\",\n          ease: Expo.easeIn\n        });\n        body.style.overflow = \"auto\";\n      }\n    };\n    return {\n      handleScroll,\n      sT,\n      rotateValue,\n      isOpen,\n      logo,\n      gnbOpen,\n      gnbTarget\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","gsap","Expo","scrollEvent","name","mixins","props","setup","sT","rotateValue","isOpen","logo","gnbTarget","handleScroll","value","window","scrollY","$el","style","transform","Match","floor","console","log","gnbOpen","body","document","to","y","ease","easeIn","overflow"],"sources":["C:\\2023\\project\\src\\components\\Layouts\\Header.vue"],"sourcesContent":["<template>\r\n    <header :class=\"isOpen ? 'open' : ''\">\r\n        <div class=\"inner\">\r\n            <h1>\r\n                <router-link to=\"/\" ref=\"logo\">R</router-link>\r\n            </h1>\r\n            <h2 class=\"title\">Creative Ui Developer</h2>\r\n            <button type=\"button\" class=\"menu\" @click=\"gnbOpen\">\r\n                <span v-if=\"!isOpen\">Menu</span>\r\n                <span v-else>Close</span>\r\n            </button>\r\n        </div>\r\n    </header>\r\n    <nav ref=\"gnbTarget\">\r\n        <div class=\"inner\">\r\n            <ul class=\"nav__list\">\r\n                <li><a href=\"javascript:;\">Home</a></li>\r\n                <li><a href=\"javascript:;\">Work</a></li>\r\n                <li><a href=\"javascript:;\">Contact</a></li>\r\n                <li><a href=\"javascript:;\">Blog</a></li>\r\n            </ul>\r\n            <ul class=\"info_list\">\r\n                <li>\r\n                    <dt>Social</dt>\r\n                    <dd>\r\n                        <a href=\"javsscript:;\">Instagram</a>\r\n                    </dd>\r\n                    <dd>\r\n                        <a href=\"javsscript:;\">Facebook</a>\r\n                    </dd>\r\n                    <dd>\r\n                        <a href=\"javsscript:;\">GitHub</a>\r\n                    </dd>\r\n                </li>\r\n                <li>\r\n                    <dt>Email</dt>\r\n                    <dd>\r\n                        <a href=\"javsscript:;\">ixkfo86@gmail.com</a>\r\n                    </dd>\r\n                </li>\r\n            </ul>\r\n        </div>\r\n\r\n    </nav>\r\n</template>\r\n  \r\n<script>\r\nimport { ref, onMounted } from 'vue';\r\nimport { gsap, Expo } from 'gsap';\r\nimport scrollEvent from '@/assets/js/scrollEvent';\r\n\r\nexport default {\r\n    name: 'Header',\r\n    mixins: [scrollEvent],\r\n    props: {\r\n    },\r\n    setup() {\r\n        const sT = ref(0);\r\n        const rotateValue = ref(0);\r\n        const isOpen = ref( false );\r\n        const logo = ref(null);\r\n        const gnbTarget = ref(null);\r\n\r\n        onMounted(() => { \r\n            handleScroll();\r\n        });\r\n\r\n        const handleScroll = () => {\r\n            sT.value = window.scrollY;\r\n            rotateValue.value = sT.value / 10;\r\n            logo.value.$el.style.transform = 'rotate(' + Match.floor(rotateValue.value) + 'deg)';\r\n            console.log( logo.value.$el.style.transform );\r\n        };\r\n\r\n   \r\n        const  gnbOpen = () => {\r\n            isOpen.value = !isOpen.value;\r\n            const body = document.body;\r\n            if( isOpen.value ){\r\n                gsap.to( gnbTarget.value, 0.05, { y: 0 , ease:Expo.easeIn });\r\n                body.style.overflow = \"hidden\";\r\n                // if( logo.value.$el.style.transform  < )\r\n                console.log( logo.value.$el.style.transform );\r\n            }else{\r\n                gsap.to( gnbTarget.value, 0.05, { y: -101+\"%\" , ease:Expo.easeIn });\r\n                body.style.overflow = \"auto\";\r\n            }\r\n        }\r\n        return {\r\n            handleScroll,\r\n            sT,\r\n            rotateValue,\r\n            isOpen,\r\n            logo,\r\n            gnbOpen,\r\n            gnbTarget\r\n        };\r\n    }\r\n}\r\n</script>\r\n  \r\n<style lang=\"scss\" scoped>\r\n@import \"@/assets/scss/__variables.scss\";\r\n\r\nheader {\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    padding: vw(30px 0 0 0);\r\n    z-index: 10;\r\n\r\n    .inner {\r\n        h1 {\r\n            position: relative;\r\n            width: vw(48px);\r\n            height: vw(48px);\r\n\r\n            a {\r\n                position: absolute;\r\n                top: 0;\r\n                left: 0;\r\n                display: flex;\r\n                width: inherit;\r\n                height: inherit;\r\n                justify-content: center;\r\n                align-items: center;\r\n                border-radius: 50%;\r\n                background: $dpColor;\r\n                overflow: hidden;\r\n                z-index: 20;