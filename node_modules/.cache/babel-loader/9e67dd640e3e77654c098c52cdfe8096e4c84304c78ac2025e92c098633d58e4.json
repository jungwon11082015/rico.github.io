   return `${year}-${month}-${day}`;\r\n        }\r\n        onMounted(() => {\r\n        });\r\n        \r\n\r\n        return {\r\n            formatDate\r\n        };\r\n    }\r\n}\r\n</script>\r\n      \r\n<style lang=\"scss\" scoped>\r\n@import \"@/assets/scss/__variables.scss\";\r\n\r\n.blog {\r\n    &--list {\r\n        @include breakDevicePoint(padding, vw(0 200px), vw(72px), null ); \r\n        @include breakDevicePoint(margin-top, vw(100px), vw(72px), null ); \r\n        >li {\r\n           border-top: 2px solid #181818;\r\n           >a{\r\n                display: flex;\r\n                justify-content: space-between;\r\n                align-items: center;\r\n                &:hover{\r\n                    .blog--title{\r\n                        &:before{\r\n                            content: '';\r\n                        }\r\n                    }\r\n                }\r\n            }\r\n            &:last-child{\r\n                border-bottom: 2px solid #181818;\r\n            }   \r\n        }\r\n    }\r\n    &--thumbnail{\r\n        width:40%;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-content: center;\r\n        padding:60px 0;\r\n        >img{\r\n            \r\n        }\r\n    }\r\n    &--context{ width: 60%; }\r\n    &--hashtag{ \r\n        display: -webkit-box;\r\n        overflow: hidden;\r\n        text-overflow: ellipsis;\r\n        max-height: 100px;\r\n        min-height: 29px;\r\n        -webkit-line-clamp: 1;\r\n        -webkit-box-orient: vertical;\r\n        margin-bottom: 20px;\r\n        >em{\r\n            display: block;\r\n            width: 100%;\r\n            font-family: 'Noto Sans Korean';\r\n            color: $dpColor;\r\n            font-size: 25px;\r\n            font-weight: 600;  \r\n            letter-spacing: -0.5px;\r\n        }\r\n    }\r\n    &--title{\r\n        display: -webkit-box;\r\n        overflow: hidden;\r\n        text-overflow: ellipsis;\r\n        max-height: