{"ast":null,"code":"import { ref, onMounted, watchEffect } from 'vue';\nimport { useRoute } from 'vue-router';\nimport { useDataStore } from '@/store/index';\nimport { storage } from '@/firebase/init.js';\nimport { ref as storageRef, getDownloadURL } from 'firebase/storage';\nimport axios from 'axios';\nimport Pagination from '@/components/Layouts/common/Pagination.vue';\nimport Skeletor from \"@/components/Layouts/common/Skeleton.vue\";\nexport default {\n  name: 'worksView',\n  props: {},\n  components: {\n    Pagination,\n    Skeletor\n  },\n  setup(props) {\n    const route = useRoute();\n    const itemsStore = useDataStore();\n    const currentItem = ref({});\n    const itemObj = ref({});\n    const hasFetchedData = ref(false);\n    const currentPage = ref(0);\n    const totalPages = ref(0);\n    const fetchWorkItems = async () => {\n      if (!hasFetchedData.value) {\n        const storageInstance = storage;\n        const jsonFileRef = storageRef(storageInstance, `works.json`);\n        try {\n          const downloadURL = await getDownloadURL(jsonFileRef);\n          const res = await axios.get(downloadURL);\n          const itemIndex = route.params.id;\n          itemObj.value = res.data;\n          currentPage.value = res.data[route.params.id - 1].id;\n          currentItem.value = res.data[route.params.id - 1];\n          totalPages.value = res.data.length - 1;\n          hasFetchedData.value = true;\n        } catch (error) {\n          console.error(\"Error fetching data:\", error);\n        }\n      }\n    };\n    watchEffect(() => {\n      const newId = Number(route.params.id);\n      console.log(typeof newId, typeof currentPage.value);\n      currentPage.value = newId;\n      console.log('route.params.id has changed:', newId);\n      fetchWorkItems();\n    });\n    onMounted(() => {\n      fetchWorkItems();\n    });\n    return {\n      currentPage,\n      hasFetchedData,\n      currentItem,\n      totalPages,\n      itemObj,\n      routeParamsId: Number(route.params.id)\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","watchEffect","useRoute","useDataStore","storage","storageRef","getDownloadURL","axios","Pagination","Skeletor","name","props","components","setup","route","itemsStore","currentItem","itemObj","hasFetchedData","currentPage","totalPages","fetchWorkItems","value","storageInstance","jsonFileRef","downloadURL","res","get","itemIndex","params","id","data","length","error","console","newId","Number","log","routeParamsId"],"sources":["C:\\Rico\\portfilo2023\\src\\views\\Work\\View.vue"],"sourcesContent":["<template>\r\n    <div id=\"work\">\r\n      <template v-if=\"hasFetchedData\">\r\n        <div class=\"work_visual\">\r\n          <img :src=\"currentItem.view?.viewBigImgSrc\" :alt=\"currentItem.project\">\r\n        </div>\r\n      </template>\r\n      <template v-else>\r\n        <Skeletor  :SkeletorType=\"`card`\"/>\r\n      </template>\r\n\r\n      <div class=\"inner\">\r\n        <section class=\"overview\">\r\n          <div class=\"text_inner\">\r\n            <div class=\"overview_title_box\">\r\n              <em class=\"title\">OVERVIEW</em> \r\n               <h2 v-dompurify-html=\"currentItem.view?.overviewTitle\"></h2>\r\n            </div>\r\n            <div class=\"overview_text_box\">\r\n              <dl v-for=\"(item, index) in currentItem.view?.websiteInfo\" :key=\"index\">\r\n                <dt >{{ item.title }}</dt>\r\n                <dd>{{ item.description }}</dd>\r\n               <!-- <dd>{{ subItem.subText }}</dd>  -->\r\n              </dl>\r\n            </div>\r\n          </div>\r\n        </section>\r\n        <section class=\"mission\">\r\n          <div class=\"text_inner\">\r\n              <em class=\"title\">MISSION</em>\r\n              <h3 v-dompurify-html=\"currentItem.view?.middleTitle\"></h3>\r\n              <img :src=\"currentItem.view?.middleBigImgSrc\" alt=\"\">\r\n          </div>\r\n        </section>\r\n\r\n        <section class=\"goals\">\r\n          <div class=\"text_inner\">\r\n            <em class=\"title\">PROJECT GOALS</em>\r\n            <ol class=\"goals_list\">\r\n              <li v-for=\"(subItem, index) in  currentItem.view?.defineList\" :key=\"index\">\r\n                <dl>\r\n                  <dt>0{{index+1 }}</dt>\r\n                  <dd v-dompurify-html=\"subItem.title\"></dd>\r\n                  <dd>{{ subItem.description}}</dd>\r\n                </dl>\r\n              </li>\r\n            </ol>\r\n          </div>\r\n    \r\n        </section>\r\n      </div>\r\n\r\n      <Pagination :item-obj=\"itemObj\"  :current-page=\"currentPage\" :total-pages=\"totalPages\" />\r\n\r\n      <!-- <Pagination\r\n        :prev-PageData=\"prevPageData\"\r\n        :next-PageData=\"nextPageData\"\r\n        @page-change=\"changePage\"\r\n      /> -->\r\n    </div>\r\n  </template>\r\n    \r\n  <script>\r\n  import { ref,  onMounted, watchEffect  } from 'vue';\r\n  import { useRoute  } from 'vue-router';\r\n  import { useDataStore } from '@/store/index'\r\n  import { storage } from '@/firebase/init.js';\r\n  import { ref as storageRef, getDownloadURL } from 'firebase/storage';\r\n  import axios from 'axios'\r\n  import Pagination from '@/components/Layouts/common/Pagination.vue'\r\n  import Skeletor from \"@/components/Layouts/common/Skeleton.vue\";\r\n  export default {\r\n    name: 'worksView',\r\n    props: {\r\n    },\r\n    components: { \r\n      Pagination,\r\n      Skeletor\r\n    },\r\n    setup(props) {\r\n      const route = useRoute();\r\n      const itemsStore = useDataStore();\r\n      const currentItem = ref({});\r\n      const itemObj = ref({});\r\n      const hasFetchedData = ref(false);\r\n      const currentPage = ref(0);\r\n      const totalPages = ref(0);\r\n\r\n      const fetchWorkItems = async () => {  \r\n        if (!hasFetchedData.value) {\r\n          const storageInstance = storage;\r\n          const jsonFileRef = storageRef(storageInstance, `works.json`);\r\n\r\n          try {\r\n              const downloadURL = await getDownloadURL(jsonFileRef);\r\n              const res = await axios.get(downloadURL);\r\n              const itemIndex = route.params.id;\r\n              itemObj.value = res.data;\r\n              currentPage.value =  res.data[route.params.id-1].id;\r\n              currentItem.value = res.data[route.params.id-1];\r\n              totalPages.value = res.data.length-1;\r\n              hasFetchedData.value = true; \r\n\r\n          } catch (error) {\r\n              console.error(\"Error fetching data:\", error);\r\n          }\r\n        }\r\n      };\r\n      watchEffect(() => {\r\n        const newId = Number(route.params.id);\r\n        console.log( typeof newId, typeof currentPage.value );\r\n        currentPage.value = newId;\r\n        console.log('route.params.id has changed:', newId);\r\n        fetchWorkItems(); \r\n      });\r\n\r\n  \r\n      onMounted(() => {\r\n        fetchWorkItems();\r\n      });\r\n\r\n      return {\r\n        currentPage,\r\n        hasFetchedData,\r\n        currentItem,\r\n        totalPages,\r\n        itemObj,\r\n        routeParamsId:  Number(route.params.id),\r\n       \r\n      };\r\n    },\r\n  }\r\n  </script>\r\n    \r\n  <style lang=\"scss\" scoped>\r\n  @import \"@/assets/scss/__variables.scss\";\r\n  #work {\r\n    .work_visual{\r\n      >img{ width: 100%; }\r\n    }\r\n    .inner{\r\n      display: block;\r\n      .text_inner{\r\n        display: flex;\r\n        padding:vw( 100px);\r\n        justify-content: space-between;\r\n        align-items:center;\r\n        .overview_title_box{\r\n          width: 55%;\r\n          .title{ \r\n            display:block;\r\n            font-size:vw(26px);\r\n            margin-bottom: vw(10px);\r\n            color: $dpColor;\r\n            font-weight: 600;\r\n          }\r\n          h2{\r\n            font-size:vw(40px);\r\n            font-family: 'Noto Sans Korean';\r\n            font-weight: bold;\r\n            line-height: 1.35;\r\n          }\r\n        }\r\n        .overview_text_box{\r\n          width: 45%;\r\n          dl{\r\n            dt{\r\n              margin-bottom: vw(16px);\r\n              font-size:vw(24px);\r\n              font-family: 'Noto Sans Korean';\r\n              font-weight: bold;\r\n            }\r\n            dd{\r\n              margin-bottom: vw(24px);\r\n              font-size:vw(22px);\r\n              font-family: 'Noto Sans Korean';\r\n              line-height: 1.35;\r\n            }\r\n          }\r\n        }\r\n      }\r\n      .mission{\r\n        display: block;\r\n        .text_inner{\r\n          display: block;\r\n          position: relative;\r\n          .title{ \r\n            display:block;\r\n            font-size:vw(26px);\r\n            padding-top: vw(120px);\r\n            margin-bottom: vw(30px);\r\n            color: $dpColor;\r\n            font-weight: 600;\r\n          }\r\n          h3{\r\n            display: inline-block;\r\n            position: relative;\r\n            font-size:vw(50px);\r\n            font-family: 'Noto Sans Korean';\r\n            font-weight: bold;\r\n            line-height: 1.2;\r\n            z-index: 2;\r\n            &:after{\r\n              content: '';\r\n              display: block;\r\n              position: absolute;\r\n              bottom:0;\r\n              left:-15%; \r\n              width: 100%;\r\n              height: 72%;\r\n              background: #fff;\r\n              z-index: -1;\r\n            }\r\n          }\r\n          img{\r\n            position: absolute;\r\n            top:0;\r\n            left:0;\r\n            width: 100%;\r\n          }\r\n        }\r\n      }\r\n      .goals{\r\n        flex-direction:row;\r\n        justify-content: space-between;\r\n        .text_inner{\r\n          padding:vw( 100px);\r\n          align-items: start;\r\n          .title{\r\n            display:block;\r\n            font-size:vw(26px);\r\n            color: $dpColor;\r\n            font-weight: 600;\r\n          }\r\n          .goals_list{\r\n            display: flex;\r\n            flex: 1;\r\n            margin-left: vw(100px);\r\n            li{\r\n              dl{\r\n             \r\n                dt{\r\n                  margin-right: vw(18px);\r\n                  float: left; \r\n                  font-size:vw(24px);\r\n                  font-weight: bold;\r\n                  color: $dpColor;\r\n                  height: vw(150px);\r\n                  &+dd{\r\n                    margin-top:vw(-8px);\r\n                    font-size:vw(30px);\r\n                    font-weight: bold;\r\n                  }\r\n                }\r\n                dd{\r\n                  margin-top: vw(14px);\r\n                  float: left;\r\n                  width: 80%;\r\n                  font-size: vw(18px);\r\n                  line-height: 1.35;\r\n                  font-family: 'Noto Sans Korean';\r\n                }\r\n                &:after{\r\n                  content: '';\r\n                  display: block;\r\n                  clear: both;\r\n                }\r\n              }\r\n            }\r\n          }\r\n\r\n        }\r\n      }\r\n    }\r\n   \r\n  }\r\n  </style> \r\n    "],"mappings":"AA+DE,SAASA,GAAG,EAAGC,SAAS,EAAEC,WAAU,QAAU,KAAK;AACnD,SAASC,QAAO,QAAU,YAAY;AACtC,SAASC,YAAW,QAAS,eAAc;AAC3C,SAASC,OAAM,QAAS,oBAAoB;AAC5C,SAASL,GAAE,IAAKM,UAAU,EAAEC,cAAa,QAAS,kBAAkB;AACpE,OAAOC,KAAI,MAAO,OAAM;AACxB,OAAOC,UAAS,MAAO,4CAA2C;AAClE,OAAOC,QAAO,MAAO,0CAA0C;AAC/D,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,KAAK,EAAE,CACP,CAAC;EACDC,UAAU,EAAE;IACVJ,UAAU;IACVC;EACF,CAAC;EACDI,KAAKA,CAACF,KAAK,EAAE;IACX,MAAMG,KAAI,GAAIZ,QAAQ,CAAC,CAAC;IACxB,MAAMa,UAAS,GAAIZ,YAAY,CAAC,CAAC;IACjC,MAAMa,WAAU,GAAIjB,GAAG,CAAC,CAAC,CAAC,CAAC;IAC3B,MAAMkB,OAAM,GAAIlB,GAAG,CAAC,CAAC,CAAC,CAAC;IACvB,MAAMmB,cAAa,GAAInB,GAAG,CAAC,KAAK,CAAC;IACjC,MAAMoB,WAAU,GAAIpB,GAAG,CAAC,CAAC,CAAC;IAC1B,MAAMqB,UAAS,GAAIrB,GAAG,CAAC,CAAC,CAAC;IAEzB,M