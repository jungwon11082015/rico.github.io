{"ast":null,"code":"import { ref, onMounted, reactive } from 'vue';\nimport { defineProps } from 'vue';\nimport { storage } from '@/firebase/init.js';\nimport { ref as storageRef, getDownloadURL } from 'firebase/storage';\nimport axios from 'axios';\nimport CardList from '@/components/Layouts/List/CardList.vue';\nimport AllWork from '@/components/Layouts/buttons/AllWork.vue';\nexport default {\n  name: 'Work',\n  defineProps: {\n    itemLength: {\n      type: Number,\n      default: 5\n    },\n    workJsonItem: {\n      type: Object,\n      required: true\n    }\n  },\n  components: {\n    CardList,\n    AllWork\n  },\n  setup() {\n    const jsonDataLoaded = ref(false);\n    const jsonData = ref(null);\n    const firebaseData = async () => {\n      const storageInstance = storage;\n      const jsonFileRef = storageRef(storageInstance, `works.json`);\n      try {\n        const downloadURL = await getDownloadURL(jsonFileRef);\n        const res = await axios.get(downloadURL);\n        const workitems = res.data;\n        console.log(workitems);\n        jsonDataLoaded.value = true;\n        jsonData.value = workitems;\n      } catch (error) {\n        console.error(\"Error fetching data:\", error);\n      }\n    };\n    onMounted(() => {\n      firebaseData();\n    });\n    return {\n      jsonDataLoaded,\n      jsonData\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","reactive","defineProps","storage","storageRef","getDownloadURL","axios","CardList","AllWork","name","itemLength","type","Number","default","workJsonItem","Object","required","components","setup","jsonDataLoaded","jsonData","firebaseData","storageInstance","jsonFileRef","downloadURL","res","get","workitems","data","console","log","value","error"],"sources":["C:\\Rico\\portfilo2023\\src\\views\\Main\\Section\\Work.vue"],"sourcesContent":["<template>\r\n    <section class=\"card\" v-show=\"jsonDataLoaded\">\r\n        <div class=\"inner\">\r\n            <h2>Project Work</h2>\r\n            <CardList v-show=\"jsonDataLoaded\" :workJsonItem=\"jsonData\" :itemLength=\"5\" />\r\n            <AllWork />\r\n        </div> \r\n    </section>\r\n</template>\r\n      \r\n<script>\r\nimport { ref, onMounted, reactive } from 'vue';\r\nimport { defineProps } from 'vue';\r\nimport { storage } from '@/firebase/init.js';\r\nimport { ref as storageRef, getDownloadURL } from 'firebase/storage';\r\nimport axios from 'axios'\r\nimport CardList from '@/components/Layouts/List/CardList.vue'\r\nimport AllWork from '@/components/Layouts/buttons/AllWork.vue'\r\n\r\nexport default {\r\n    name: 'Work',\r\n    defineProps: {\r\n        itemLength: {\r\n            type: Number,\r\n            default: 5\r\n        },\r\n        workJsonItem: {\r\n            type: Object,\r\n            required: true\r\n        }\r\n    },\r\n    components: {\r\n        CardList,\r\n        AllWork\r\n    },\r\n\r\n    setup() {\r\n        const jsonDataLoaded = ref( false ); \r\n        const jsonData = ref(null);\r\n        const firebaseData = async () => {\r\n            const storageInstance = storage;\r\n            const jsonFileRef = storageRef(storageInstance, `works.json`);\r\n            try {\r\n                const downloadURL = await getDownloadURL(jsonFileRef);\r\n                const res = await axios.get( downloadURL );\r\n                const workitems = res.data; \r\n                console.log( workitems );\r\n                jsonDataLoaded.value = true;\r\n                jsonData.value = workitems;\r\n                \r\n            } catch (error) {\r\n                console.error(\"Error fetching data:\", error);\r\n            }\r\n        } \r\n        onMounted(() => {\r\n            firebaseData();\r\n        })\r\n        return {\r\n            jsonDataLoaded,\r\n            jsonData\r\n        };\r\n    }\r\n}\r\n</script>\r\n      \r\n<style lang=\"scss\" scoped>\r\n@import \"@/assets/scss/__variables.scss\";\r\n\r\nsection {\r\n    padding: vw(100px 0)