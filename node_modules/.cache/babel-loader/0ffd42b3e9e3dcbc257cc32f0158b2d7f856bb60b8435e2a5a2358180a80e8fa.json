{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport { gsap, power3 } from 'gsap';\nexport default {\n  name: 'cursor',\n  props: {},\n  setup() {\n    const cursorPointer = ref(null);\n    onMounted(() => {\n      gsap.set(cursorPointer.value, {\n        force3D: true\n      });\n    });\n    function handleMouseMove(event) {\n      console.log(event);\n      let x = event.clientX;\n      let y = event.clientY;\n      console.log(x, y);\n      gsap.to(cursorPointer.value, {\n        x: x - 16,\n        y: y - 16,\n        ease: 'power3'\n      });\n    }\n    function handleMouseLeave(event) {\n      gsap.to(cursorPointer.value, {\n        scale: 0,\n        duration: 0.1,\n        ease: 'none'\n      });\n    }\n    function handleMouseEnter(event) {\n      gsap.to(cursorPointer.value, {\n        scale: 1,\n        duration: 0.1,\n        ease: 'none'\n      });\n    }\n    return {\n      cursorPointer,\n      handleMouseMove,\n      handleMouseLeave,\n      handleMouseEnter\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","gsap","power3","name","props","setup","cursorPointer","set","value","force3D","handleMouseMove","event","console","log","x","clientX","y","clientY","to","ease","handleMouseLeave","scale","duration","handleMouseEnter"],"sources":["C:\\2023\\project\\src\\components\\Mouse\\Cursor.vue"],"sourcesContent":["<template>\r\n  <div\r\n    class=\"cursor\"\r\n    ref=\"cursorPointer\"\r\n    @mousemove=\"handleMouseMove\"\r\n    @mouseleave=\"handleMouseLeave\"\r\n    @mouseenter=\"handleMouseEnter\"\r\n  ></div>\r\n</template>\r\n  \r\n<script>\r\nimport { ref, onMounted } from 'vue';\r\nimport { gsap, power3 } from 'gsap';\r\nexport default {\r\n\r\n    name: 'cursor',\r\n    props: {\r\n    },\r\n\r\n    setup() {\r\n        const cursorPointer = ref(null);\r\n\r\n        onMounted(() => {\r\n            gsap.set(cursorPointer.value, { force3D: true });\r\n        });\r\n\r\n        function handleMouseMove(event) {\r\n            console.log( event );\r\n            let x = event.clientX;\r\n            let y = event.clientY;\r\n            console.log(x, y);\r\n            gsap.to(cursorPointer.value, {\r\n                x: x - 16,\r\n                y: y - 16,\r\n                ease: 'power3',\r\n            });\r\n        }\r\n\r\n        function handleMouseLeave(event) {\r\n            gsap.to(cursorPointer.value, {\r\n                scale: 0,\r\n                duration: 0.1,\r\n                ease: 'none',\r\n            });\r\n        }\r\n\r\n        function handleMouseEnter(event) {\r\n            gsap.to(cursorPointer.value, {\r\n                scale: 1,\r\n                duration: 0.1,\r\n                ease: 'none',\r\n            });\r\n        }\r\n\r\n        return {\r\n            cursorPointer,\r\n            handleMouseMove,\r\n            handleMouseLeave,\r\n            handleMouseEnter,\r\n        };\r\n    },\r\n\r\n\r\n}\r\n</script>\r\n  \r\n<style lang=\"scss\" scoped>\r\n.cursor {\r\n    border-radius: 50%;\r\n    width: 32px;\r\n    height: 32px;\r\n    background-color: white;\r\n    mix-blend-mode: difference;\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    will-change: transform;\r\n    pointer-events: none;\r\n}\r\n</style>\r\n  "],"mappings":"AAWA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,IAAI,EAAEC,MAAK,QAAS,MAAM;AACnC,eAAe;EAEXC,IAAI,EAAE,QAAQ;EACdC,KAAK,EAAE,CACP,CAAC;EAEDC,KAAKA,CAAA,EAAG;IACJ,MAAMC,aAAY,GAAIP,GAAG,CAAC,IAAI,CAAC;IAE/BC,SAAS,CAAC,MAAM;MACZC,IAAI,CAACM,GAAG,CAACD,aAAa,CAACE,KAAK,EAAE;QAAEC,OAAO,EAAE;MAAK,CAAC,CAAC;IACpD,CAAC,CAAC;IAEF,SAASC,eAAeA,CAACC,KAAK,EAAE;MAC5BC,OAAO,CAACC,GAAG,CAAEF,KAAM,CAAC;MACpB,IAAIG,CAAA,GAAIH,KAAK,CAACI,OAAO;MACrB,IAAIC,CAAA,GAAIL,KAAK,CAACM,OAAO;MACrBL,OAAO,CAACC,GAAG,CAACC,CAAC,EAAEE,CAAC,CAAC;MACjBf,IAAI,CAACiB,EAAE,CAACZ,aAAa,CAACE,KAAK,EAAE;QACzBM,CAAC,EAAEA,CAAA,GAAI,EAAE;QACTE,CAAC,EAAEA,CAAA,GAAI,EAAE;QACTG,IAAI,EAAE;MACV,CAAC,CAAC;IACN;IAEA,SAASC,gBAAgBA,CAACT,KAAK,