onData.view?.defineList\" :key=\"index\">\r\n                <dl>\r\n                  <dt>0{{index+1 }}</dt>\r\n                  <dd v-dompurify-html=\"subItem.title\"></dd>\r\n                  <dd>{{ subItem.description}}</dd>\r\n                </dl>\r\n              </li>\r\n            </ol>\r\n          </div>\r\n    \r\n        </section>\r\n      </div>\r\n      <Pagination\r\n        :current-page=\"currentPage\"\r\n        :total-pages=\"totalPages\"\r\n        :prev-PageData=\"prevPageData\"\r\n        :next-PageData=\"nextPageData\"\r\n        @page-change=\"changePage\"\r\n      />\r\n    </div>\r\n  </template>\r\n    \r\n  <script>\r\n  import { ref, reactive, onMounted, computed , watch } from 'vue';\r\n  import { useRoute, useRouter  } from 'vue-router';\r\n  import { storage } from '@/firebase/init.js';\r\n  import { ref as storageRef, getDownloadURL } from 'firebase/storage';\r\n  import axios from 'axios'\r\n  import Pagination from '@/components/Layouts/common/Pagination.vue'\r\n  \r\n  export default {\r\n    name: 'worksView',\r\n    props: {\r\n    },\r\n    components: { \r\n      Pagination\r\n    },\r\n    setup(props) {\r\n      const route = useRoute();\r\n      const router = useRouter();\r\n\r\n      const jsonData = ref({});\r\n      const currentPage = ref( 0 );\r\n      const totalPages = ref( 0 );\r\n      const prevPageData = ref({});\r\n      const nextPageData = ref({});\r\n \r\n\r\n      const fetchWorkItems = async () => {  \r\n          const storageInstance = storage;\r\n          const jsonFileRef = storageRef(storageInstance, `works.json`);\r\n\r\n          try {\r\n              const downloadURL = await getDownloadURL(jsonFileRef);\r\n              const res = await axios.get(downloadURL);\r\n              const workitems = res.data;\r\n              const curtPage = workitems[route.params.id];\r\n              prevPageData.value =  workitems[ Number(route.params.id)-1];\r\n              nextPageData.value =  workitems[ Number(route.params.id)];\r\n              totalPages.value = workitems.length;\r\n              jsonData.value = workitems[route.params.id];\r\n              currentViewPage(curtPage.id-1);\r\n              currentPage.value = workitems[route.params.id].id - 1;\r\n\r\n          } catch (error) {\r\n              console.error(\"Error fetching data:\", error);\r\n          }\r\n      };\r\n\r\n      const changePage = ( page ) => {\r\n        console.log( \"changePage\", page )\r\n        currentPage.value = page;\r\n      }\r\n      \r\n      const currentViewPage = (curtPage) =>{\r\n        currentPage.value = curtPage;\r\n\r\n      }\r\n\r\n      onMounted(() => {\r\n        fetchWorkItems();\r\n        currentViewPage();\r\n      });\r\n\r\n      return {\r\n        jsonData,\r\n        routeParamsId:  Number(route.params.id),\r\n        currentPage,\r\n        totalPages,\r\n        prevPageData,\r\n        nextPageData,\r\n        changePage\r\n      };\r\n    },\r\n  }\r\n  </script>\r\n    \r\n  <style lang=\"scss\" scoped>\r\n  @import \"@/assets/scss/__variables.scss\";\r\n  #work {\r\n    .work_visual{\r\n      >img{ width: 100%; }\r\n    }\r\n    .inner{\r\n      display: block;\r\n      .text_inner{\r\n        display: flex;\r\n        padding:vw( 100px);\r\n        justify-content: space-between;\r\n        align-items:center;\r\n        .overview_title_box{\r\n          width: 55%;\r\n          .title{ \r\n            display:block;\r\n            font-size:vw(26px);\r\n            margin-bottom: vw(10px);\r\n            color: $dpColor;\r\n            font-weight: 600;\r\n          }\r\n          h2{\r\n            font-size:vw(40px);\r\n            font-family: 'Noto Sans Korean';\r\n            font-weight: bold;\r\n            line-height: 1.35;\r\n          }\r\n        }\r\n        .overview_text_box{\r\n          width: 45%;\r\n          dl{\r\n            dt{\r\n              margin-bottom: vw(16px);\r\n              font-size:vw(24px);\r\n 