s = await axios.get( downloadURL );\r\n                const workitems = res.data;\r\n                jsonData.value =  workitems;\r\n            } catch (error) {\r\n                console.error(\"Error fetching data:\", error);\r\n            }\r\n        } \r\n        const videoDimds = () => {\r\n            const gnbItems = gsap.utils.toArray(\".video__dimd\");\r\n            console.log( gnbItems, gnbItems.length );\r\n            gnbItems.forEach((item) => {\r\n                let tl = gsap.timeline({\r\n                    scrollTrigger: {\r\n                        trigger: item,\r\n                    }\r\n                });\r\n                tl.set(item, { autoAlpha: 1, left: 0 });\r\n                tl.to(item, 0.75, {\r\n                    left: 100 + \"%\",\r\n                    ease: Power2.out\r\n                });\r\n            })\r\n        }\r\n        onUpdated(() => {\r\n            videoDimds();\r\n        });\r\n\r\n        onMounted(() => {\r\n            firebaseData();\r\n        })\r\n        return {\r\n            jsonData,\r\n            videoDimds,\r\n        };\r\n    }\r\n}\r\n</script>\r\n      \r\n<style lang=\"scss\" scoped>\r\n@import \"@/assets/scss/__variables.scss\";\r\n\r\n.card {\r\n    &--list {\r\n        >li {\r\n            position: relative;\r\n            margin: vw(0 0 50px 0);\r\n            border-radius: vw(15px);\r\n            overflow: hidden;\r\n            height: vw(750px);\r\n\r\n            a {\r\n                display: block;\r\n                height: inherit;\r\n\r\n                .video__dimd {\r\n                    position: absolute;\r\n                    top: 0;\r\n                    left: 0;\r\n                    width: 100%;\r\n                    height: 100%;\r\n                    background: #fff;\r\n                    z-index: 5;\r\n                }\r\n\r\n                video {\r\n                    width: 100%;\r\n                    height: 100%;\r\n                    ob