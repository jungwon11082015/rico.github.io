{"ast":null,"code":"import BlogList from '@/components/Layouts/List/BlogList.vue';\nimport { ref, reactive, onMounted } from 'vue';\nimport axios from 'axios';\nexport default {\n  name: 'Blog',\n  props: {},\n  components: {\n    BlogList\n  },\n  setup() {\n    const ACCESS_TOKEN = \"88f0ff4e5c7f1fca1ceee0437e6771d5_e4db11b9a579e41ca6dde90025c7e146\";\n    const objLayout = async () => {\n      try {\n        const response = await axios.get(\"https://www.tistory.com/apis/blog/info\", {\n          params: {\n            access_token: ACCESS_TOKEN,\n            output: \"json\",\n            blogName: 'Rico',\n            page: '1'\n          }\n        });\n        // Handle the response data\n        console.log(response.data);\n      } catch (error) {\n        // Handle errors \n        console.error('An error occurred:', error);\n      }\n    };\n    onMounted(() => {\n      objLayout();\n    });\n    return {\n      objLayout\n    };\n  }\n};","map":{"version":3,"names":["BlogList","ref","reactive","onMounted","axios","name","props","components","setup","ACCESS_TOKEN","objLayout","response","get","params","access_token","output","blogName","page","console","log","data","error"],"sources":["C:\\Rico\\portfilo2023\\src\\views\\Blog.vue"],"sourcesContent":["<template>\r\n    <section id=\"blog\">\r\n        <div class=\"inner\">\r\n            <h2>BLOG</h2>\r\n            <p class=\"blog--word\">현재 등록된 글 <em>10</em>개입니다.</p>\r\n            <BlogList />\r\n        </div>\r\n    </section>\r\n</template>\r\n  \r\n<script>\r\nimport BlogList from '@/components/Layouts/List/BlogList.vue'\r\nimport { ref, reactive, onMounted } from 'vue';\r\nimport axios from 'axios'\r\n\r\n\r\nexport default {\r\n    name: 'Blog',\r\n    props: {\r\n    },\r\n    components: {\r\n        BlogList\r\n    },\r\n    setup() {\r\n        const ACCESS_TOKEN = \"88f0ff4e5c7f1fca1ceee0437e6771d5_e4db11b9a579e41ca6dde90025c7e146\";\r\n\r\n        const objLayout = async () => {\r\n\r\n            \r\n            try {\r\n                const response = await axios.get(\"https://www.tistory.com/apis/blog/info\", {\r\n                    params: {\r\n                    access_token: ACCESS_TOKEN,\r\n                    output: \"json\",\r\n                    blogName: 'Rico',\r\n                    page: '1',\r\n                    },\r\n                })\r\n                // Handle the response data\r\n                console.log(response.data);\r\n            } catch (error) {\r\n                // Handle errors \r\n                console.error('An error occurred:', error);\r\n            }\r\n        };\r\n        \r\n        onMounted(() => {\r\n            objLayout();\r\n        });\r\n        \r\n        return {\r\n            objLayout\r\n        };\r\n    }\r\n}\r\n</script>\r\n  \r\n<style lang=\"scss\" scoped>\r\n@import \"@/assets/scss/__variables.scss\";\r\n\r\n#blog {\r\n    .inner {\r\n        display: block;\r\n\r\n        h2 {\r\n            padding-top: 150px;\r\n            text-align: center;\r\n            font-weight: bold;\r\n            font-size: 120px;\r\n            color: $dpColor;\r\n        }\r\n\r\n        .blog--word {\r\n            padding: 20px 0;\r\n            text-align: center;\r\n            font-size: 24px;\r\n            font-family: 'Noto Sans Korean';\r\n            font-weight: 600;\r\n            color: #181818;\r\n\r\n            >em {\r\n                font-size: 24px;\r\n                color: $dpColor;\r\n\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n  "],"mappings":"AAWA,OAAOA,QAAO,MAAO,wCAAuC;AAC5D,SAASC,GAAG,EAAEC,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AAC9C,OAAOC,KAAI,MAAO,OAAM;AAGxB,eAAe;EACXC,IAAI,EAAE,MAAM;EACZC,KAAK,EAAE,CACP,CAAC;EACDC,UAAU,EAAE;IACRP;EACJ,CAAC;EACDQ,KAAKA,CAAA,EAAG;IACJ,MAAMC,YAAW,GAAI,mEAAmE;IAExF,MAAMC,SAAQ,GAAI,MAAAA,CAAA,KAAY;MAG1B,IAAI;QACA,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,GAAG,CAAC,wCAAwC,EAAE;UACvEC,MAAM,EAAE;YACRC,YAAY,EAAEL,YAAY;YAC1BM,MAAM,EAAE,MAAM;YACdC,QAAQ