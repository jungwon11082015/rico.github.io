{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { reactive, ref, onMounted } from 'vue';\nimport { gsap, Power2 } from 'gsap';\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\ngsap.registerPlugin(ScrollTrigger);\nexport default {\n  name: 'CardList',\n  props: {\n    itemLength: {\n      type: Number\n    },\n    workJsonItem: {\n      type: Array,\n      required: true\n    }\n  },\n  setup(props) {\n    const workItems = reactive([]);\n    onMounted(() => {\n      setInterval(() => {\n        workItems.push(...props.workJsonItem);\n        console.log(workItems);\n      }, 1000);\n      const gnbItems = gsap.utils.toArray(\".video__dimd\");\n      gnbItems.forEach(item => {\n        let tl = gsap.timeline({\n          scrollTrigger: {\n            trigger: item\n          }\n        });\n        tl.set(item, {\n          autoAlpha: 1,\n          left: 0\n        });\n        tl.to(item, 0.75, {\n          left: 100 + \"%\",\n          ease: Power2.out\n        });\n      });\n    });\n    return {\n      workItems\n    };\n  }\n};","map":{"version":3,"names":["reactive","ref","onMounted","gsap","Power2","ScrollTrigger","registerPlugin","name","props","itemLength","type","Number","workJsonItem","Array","required","setup","workItems","setInterval","push","console","log","gnbItems","utils","toArray","forEach","item","tl","timeline","scrollTrigger","trigger","set","autoAlpha","left","to","ease","out"],"sources":["C:\\Rico\\portfilo2023\\src\\components\\Layouts\\List\\CardList.vue"],"sourcesContent":["<template>\r\n    <ul class=\"card--list\">\r\n        {{ workItems }}\r\n        <li class=\"card__item\" v-for=\"(item, index) in workItems.slice(0, itemLength)\" :key=\"index\">\r\n            <router-link :to=\"{name: 'WorkView', params: { id: item.id }}\">\r\n                <div class=\"video__dimd\"></div>\r\n                <video loop playsinline muted autoplay>\r\n                    <source :src=\"item.dataSrc\" type=\"video/mp4\">\r\n                </video>\r\n                <ul class=\"card--tag__list\" >\r\n                    <li>{{ item.skill[0] }}</li>\r\n                    <li>{{ item.skill[1] }}</li>\r\n                </ul>\r\n            </router-link>>\r\n        </li> \r\n    </ul>\r\n</template>\r\n      \r\n<script>\r\nimport { reactive, ref, onMounted  } from 'vue';\r\nimport { gsap, Power2 } from 'gsap';\r\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\r\ngsap.registerPlugin(ScrollTrigger);\r\n\r\nexport default {\r\n    name: 'CardList',\r\n    props: {\r\n        itemLength: {\r\n            type: Number,\r\n        },\r\n        workJsonItem: {\r\n            type: Array,\r\n            required: true\r\n        },\r\n    },\r\n    setup(props) {\r\n\r\n        const workItems = reactive([]);\r\n\r\n        onMounted(() => {\r\n            \r\n            setInterval(() => {\r\n                workItems.push(...props.workJsonItem);\r\n                console.log(workItems );\r\n            }, 1000);\r\n            const gnbItems = gsap.utils.toArray(\".video__dimd\");\r\n            gnbItems.forEach((item) => {\r\n                let tl = gsap.timeline({\r\n                    scrollTrigger: {\r\n                        trigger: item,\r\n                    }\r\n                });\r\n                tl.set(item, { autoAlpha: 1, left: 0 });\r\n                tl.to(item, 0.75, {\r\n                    left: 100 + \"%\",\r\n                    ease: Power2.out\r\n                });\r\n            })\r\n        })\r\n        return {\r\n            workItems,\r\n        };\r\n    }\r\n}\r\n</script>\r\n      \r\n<style lang=\"scss\" scoped>\r\n@import \"@/assets/scss/__variables.scss\";\r\n\r\n.card {\r\n    &--list {\r\n        >li {\r\n            position: relative;\r\n            margin: vw(0 0 50px 0);\r\n            border-radius: vw(15px);\r\n            overflow: hidden;\r\n            height: vw(750px);\r\n\r\n            a {\r\n                display: block;\r\n                height: inherit;\r\n\r\n                .video__dimd {\r\n                    position: absolute;\r\n                    top: 0;\r\n                    left: 0;\r\n                    width: 100%;\r\n                    height: 100%;\r\n                    background: #fff;\r\n                    z-index: 5;\r\n                }\r\n\r\n                video {\r\n                    width: 100%;\r\n                    height: 100%;\r\n                    object-fit: cover;\r\n                    pointer-events: none;\r\n                    position: absolute;\r\n                    top: 0;\r\n                    left: 0;\r\n                    z-index: 0;\r\n                }\r\n            }\r\n        }\r\n    }\r\n\r\n    &--tag__list {\r\n        position: absolute;\r\n        bottom: vw(50px);\r\n        left: vw(30px);\r\n        display: flex;\r\n\r\n        >li {\r\n            border-radius: vw(30px);\r\n            font-size: vw(40px);\r\n            padding: vw(14px 30px);\r\n            margin-left: vw(20px);\r\n            background: #fff;\r\n            color: $dpColor;\r\n            font-weight: 600;\r\n\r\n            &:first-child {\r\n                margin-left: 0;\r\n            }\r\n\r\n            &:nth-child(odd) {\r\n                background: $dpColor;\r\n                color: #fff;\r\n            }\r\n        }\r\n    }\r\n}\r\n\r\n@media (max-width: 960px) {\r\n    .card {\r\n        &--tag__list {\r\n            >li{\r\n                font-size:vw(60px);\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n      "],"mappings":";AAmBA,SAASA,QAAQ,EAAEC,GAAG,EAAEC,SAAQ,QAAU,KAAK;AAC/C,SAASC,IAAI,EAAEC,MAAK,QAAS,MAAM;AACnC,SAASC,aAAY,QAAS,oBAAoB;AAClDF,IAAI,CAACG,cAAc,CAACD,aAAa,CAAC;AAElC,eAAe;EACXE,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE;IACHC,UAAU,EAAE;MACRC,IAAI,EAAEC;IACV,CAAC;IACDC,YAAY,EAAE;MACVF,IAAI,EAAEG,KAAK;MACXC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDC,KAAKA,CAACP,KAAK,EAAE;IAET,MAAMQ,SAAQ,GAAIhB,QAAQ,CAAC,EAAE,CA