{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport { gsap, Expo, Power2, Linear } from 'gsap';\nimport { ScrollTrigger } from 'gsap/ScrollTrigger';\nimport CardList from '@/components/List/CardList.vue';\ngsap.registerPlugin(ScrollTrigger);\nexport default {\n  name: 'WorkList',\n  props: {},\n  components: {\n    CardList\n  },\n  setup() {\n    const title = ref(null);\n    const subTitle = ref(null);\n    const count = ref(0);\n    const game = {\n      score: 0\n    };\n    const titleShow = () => {\n      const items = gsap.utils.toArray(\".js-motion\");\n      gsap.set(items, {\n        opacity: 0,\n        y: 50\n      });\n      gsap.to(items, 0.75, {\n        opacity: 1,\n        y: 0,\n        ease: Expo.easeInOut,\n        onComplete: () => {\n          gsap.to(game, 4, {\n            score: \"+=58\",\n            roundProps: \"score\",\n            ease: Linear.easeNone,\n            onUpdate: upDateHandler\n          });\n        }\n      });\n      items.forEach(item => {\n        let tl = gsap.timeline({\n          scrollTrigger: {\n            trigger: item,\n            start: \"top  center\",\n            // markers:true,\n            scrub: true\n          }\n        });\n        tl.set(item, {\n          autoAlpha: 1\n        });\n        tl.to(item, 0.75, {\n          autoAlpha: 0,\n          ease: Power2.out\n        });\n      });\n    };\n    const upDateHandler = () => {\n      count.value.innerText = game.score;\n    };\n    onMounted(() => {\n      titleShow();\n    });\n    return {\n      title,\n      subTitle,\n      count\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","gsap","Expo","Power2","Linear","ScrollTrigger","CardList","registerPlugin","name","props","components","setup","title","subTitle","count","game","score","titleShow","items","utils","toArray","set","opacity","y","to","ease","easeInOut","onComplete","roundProps","easeNone","onUpdate","upDateHandler","forEach","item","tl","timeline","scrollT