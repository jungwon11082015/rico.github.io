            autoAlpha:0,\r\n            ease: Power2.out\r\n        });\r\n      });\r\n\r\n    \r\n    }\r\n\r\n    const upDateHandler = () => {\r\n      count.value.innerText = game.score;\r\n    }\r\n\r\n    onMounted(() => {\r\n      titleShow();\r\n    });\r\n\r\n\r\n    return {\r\n      title,\r\n      subTitle,\r\n      count\r\n    };\r\n  }\r\n}\r\n</script>\r\n    \r\n<style lang=\"scss\" scoped>\r\n@import \"@/assets/scss/__variables.scss\";\r\n$bg-width: 50px;\r\n$bg-height: 50px;\r\n\r\n#work {\r\n  .inner {\r\n    display: block;\r\n    .work_top_content{\r\n      background: url(data:image/png;base64,) repeat 0 0;\r\n      -webkit-animation: bg-scrolling-reverse 0.92s infinite;\r\n      -moz-animation: bg-scrolling-reverse 0.92s infinite;\r\n      -o-animation: bg-scrolling-reverse 0.92s infinite;\r\n      animation: bg-scrolling-reverse 0.92s infinite;\r\n      -webkit-animation-timing-function: linear;\r\n      -moz-animation-timing-function: linear;\r\n      -o-animation-timing-function: linear;\r\n      animation-timing-function: linear;\r\n\r\n      .work_text_box{\r\n        padding: vw(400px 0);\r\n        display: flex;\r\n        justify-content: space-between;\r\n        align-items: center;\r\n      }\r\n      \r\n      h2 {\r\n        width: 45%;\r\n        font-size: vw(80px);\r\n        color: #141414;\r\n        font-weight: bold;\r\n        letter-spacing: vw(-3px);\r\n        line-height: 1.3;\r\n        font-family: 'Noto Sans Korean';\r\n      }\r\n\r\n      h3 {\r\n        width: 55%;\r\n        font-size: vw(80px);\r\n        color: #141414;\r\n        font-weight: bold;\r\n        letter-spacing: vw(-3px);\r\n        line-height: 1.3;\r\n        font-family: 'Noto Sans Korean';\r\n        .count{\r\n          display: inline-block;\r\n          text-align: center;\r\n          font-size: vw(80px);\r\n          width: vw(100px);\r\n          color: $dpColor;\r\n        }\r\n      }\r\n\r\n      @keyframes         bg-scrolling-reverse {\r\n          100% { background-position: $bg-width $bg-height; }\r\n        }\r\n\r\n    }\r\n \r\n  }\r\n}\r\n\r\n\r\n@media (max-width: 960px) {\r\n  #work {\r\n    .inner{\r\n      .work_top_content{\r\n        .work_text_box{\r\n          padding:vw(200px 0 150px);\r\n          flex-direction:column;\r\n        }\r\n        h2,h3{\r\n          width: 100%;\r\n          font-size:vw(100px);\r\n        }\r\n        h2{\r\n          br{ display: none; }\r\n        }\r\n        h3{\r\n          .count{ font-size:vw(100px)}\r\n        }\r\n      }\r\n    }\r\n  \r\n  }\r\n}\r\n\r\n\r\n</style>\r\n    "],"mappings":"AAoBA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,IAAI,EAAEC,IAAI,EAAEC,MAAM,EAAEC,MAAK,QAAS,MAAM;AACjD,SAASC,aAAY,QAAS,oBAAoB;AAClD,OAAOC,QAAO,MAAO,gCAA+B;AACpDL,IAAI,CAACM,cAAc,CAACF,aAAa,CAAC;AAGlC,eAAe;EACbG,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE,CACP,CAAC;EACDC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIb,GAAG,CAAC,IAAI,CAAC;IACvB,MAAMc,QAAO,GAAId,GAAG,CAAC,IAAI,CAAC;IAC1B,MAAMe,KAAI,GAAIf,GAAG,CAAE,CAAE,CAAC;IACtB,MAAMgB,IAAG,GAAI;MAACC,KAAK,EAAC;IAAC,CAAC;IAEtB,MAAMC,SAAQ,GAAKA,CAAA,KAAK;MAEtB,MAAMC,KAAI,GAAIjB,IAAI,CAACkB,KAAK,CAACC,OAAO,CAAC,YAAY,CAAC;MAC9CnB,IAAI,CAACoB,GAAG,CAAGH,KAAK,EAAE;QAAEI,OAAO,EAAE,CAAC;QAAEC,CAAC,EAAC;MAAG,CAAC,CAAC;MACrCtB,IAAI,CAACuB,EAAE,CAAGN,KAAK,EAAE,IAAI,EAAE;QAAEI,OAAO,EAAE,CAAC;QAAEC,CAAC,EAAC,CAAC;QAAEE,IAAI,EAACvB,IAAI,CAACwB,SAAS;QAAEC,UAAU,EAAEA,CAAA,KAAM;UAC/E1B,IAAI,CAACuB,EAAE,CAAET,IAAI,EAAE,CAAC,EAAE;YAACC,KAAK,EAAC,MAAM;YAAEY,UAAU,EAAC,OAAO;YAAEH,IAAI,EAACrB,MAAM,CAACyB,QAAQ;YAAEC,QAAQ,EAACC;UAAa,CAAC,CAAC;QACvG;MAAC,CAAC,CAAC;MAEHb,KAAK,CAACc,OAAO,CAAGC,IAAI,IAAI;QACtB,IAAIC,EAAC,GAAIjC,IAAI,CAACkC,QAAQ,CAAC;UACnBC,aAAa,EAAE;YACXC,OAAO,EAAEJ,IAAI;YACbK,KAAK,EAAE,aAAa;YACpB;YACAC,KAAK,EAAE;UAEX;QACJ,CAAC,CAAC;QACFL,EAAE,CAACb,GAAG,CAACY,IAAI,EAAE;UAAEO,SAAS,EAAE;QAAG,CAAC,CAAC;QAC/BN,EAAE,CAACV,EAAE,CAACS,IAAI,EAAE,IAAI,EAAE;UACdO,SAAS,EAAC,CAAC;UACXf,IAAI,EAAEtB,MAAM,CAACsC;Q